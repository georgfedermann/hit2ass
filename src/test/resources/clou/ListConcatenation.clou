This is some fixed text for you.
#L liste99 D { 1, 2, 3, 4, 5 }
#L liste98 D { "Die", "fiend", "die" }
#L liste97 D { "Horrido" } & { "auf" } & { "und" } & { "der" } & { "Gas" } & { "nach" }
#L liste97 & liste89 & liste99 & { 99, 98 } & { 96, 30 }

#^2"RETURN"
